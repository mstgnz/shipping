{
    "name": "mstgnz/cargo",
    "description": "Cargo Integration",
    "type": "library",
    "homepage": "https://github.com/mstgnz/cargo",
    "keywords": ["Cargo","Integration"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mesut GENEZ",
            "email": "mesutgenez@hotmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "MstGnz\\Cargo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MstGnz\\Cargo\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
